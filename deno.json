{
    "lint": {
      "include": ["src/"],
      "exclude": [],
      "rules": {
        "exclude": ["no-unused-vars"]
      }
    },
    "fmt": {
        "useTabs": true,
        "lineWidth": 80,
        "indentWidth": 4,
        "semiColons": true,
        "singleQuote": true,
        "proseWrap": "preserve",
        "include": ["src/"],
        "exclude": []
      },
      "lock": {
        "path": "./deno.lock",
        "frozen": true
      },
      "nodeModulesDir": "auto",
      "compilerOptions": {
        "target": "es2023"
      }
  }
